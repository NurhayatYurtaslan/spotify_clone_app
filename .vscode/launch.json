    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "[DEV] spotify",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "program": "lib/flavors/main_dev.dart",
            "args": [
                "--flavor",
                "dev",
                "--target",
                "lib/flavors/main_dev.dart"
            ]
        },
        {
            "name": "[Prod] spotify",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "program": "lib/flavors/main_prod.dart",
            "args": [
                "--flavor",
                "prod",
                "--target",
                "lib/flavors/main_prod.dart"

            ]
        }
    ]
}